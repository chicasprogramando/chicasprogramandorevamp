    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.3.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^4.2.2",
    "@vue/cli-service": "^3.9.0",
